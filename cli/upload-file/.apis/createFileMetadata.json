{
    "Parameters": {
        "urlPrefix": "http://116.198.204.139:5001/",
        "loginJsonPath": "$`${env('userprofile')}/.easyplm/cache/login.json`",
        "loginJson": "$Json.Load(loginJsonPath)",
        "token": "$loginJson.token",
        "fileSize": "",
        "fileName": "",
        "bucketName": ""
    },
    "Method": "POST",
    "Url": "$urlPrefix+`pdm-standalone/file-micro/file/createFileMetadata`",
    "Headers": {
        "accesstoken": "$token",
        "appName": "easyplm"
    },
    "Body": {
        "fileSize": "$fileSize",
        "fileName": "$fileName",
        "bucketName": "$bucketName"
    }
}