{
    "Parameters": {
        "url": "",
        "loginJsonPath": "$`${env('userprofile')}/.easyplm/cache/login.json`",
        "loginJson": "$Json.Load(loginJsonPath)",
        "token": "$loginJson.token",
        "filePath": ""
    },
    "Method": "PUT",
    "Url": "$url",
    "Headers": {
        "accesstoken": "$token",
        "appName": "easyplm"
    },
    "Body": {
        "Type": "Binary",
        "Path": "$filePath"
    }
}